<div class="container con-form">
  <div class="d-flex justify-content-center h-100">
    <div class="card card-body shadow">
      <div class="head">
        <h1><i class="fas fa-user-shield"></i> Register</h1>
      </div>
      <%- include("../partials/messages") %>
      <div class="form">
        <form action="/admin/resident-account" method="POST">
          <div class="form-group">
            <select class="form-control" name="resident" id="resident">
              <% resident.forEach(res => { %>
              <option value="<%= res._id%>|<%= res.full_name%>"
                ><%= res.full_name%></option
              >
              <% }) %>
            </select>
          </div>
          <div class="form-group">
            <label for="email">Email</label>
            <input class="form-control" type="email" name="email" id="email" />
          </div>
          <div class="form-group">
            <label for="passsword">passsword</label>
            <input
              class="form-control"
              type="password"
              name="password"
              id="password"
            />
          </div>
          <div class="form-group">
            <label for="password2">Re-Password</label>
            <input
              class="form-control"
              type="password"
              name="password2"
              id="password2"
            />
          </div>
          <button
            class="btn btn-primary btn-block"
            data-toggle="modal"
            data-target="#myModal"
          >
            Register
          </button>
        </form>
      </div>
    </div>
  </div>
</div>
<hr />
